{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "template of vue with webpack4",
  "main": "src/main.js",
  "scripts": {
    "build:prod": "node build/build.js --env.NODE_ENV=production --env.PACKAGE_ENV=prod",
    "build:test": "node build/build.js --env.NODE_ENV=production --env.PACKAGE_ENV=test",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.config.js",
    "dev:mock": "webpack-dev-server --inline --progress --config build/webpack.config.js --env.API_ENV=mock",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix"
  },
  "author": "sfeng",
  "license": "ISC",
  "dependencies": {
    "echarts": "^4.2.1",
    "element-ui": "^2.9.2",
    "jspdf": "^2.3.1",
    "jspdf-customfonts": "^0.0.4-rc.4",
    "vant": "^2.0.2",
    "vue": "^2.6.10",
    "vue-baidu-map": "^0.21.22",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
